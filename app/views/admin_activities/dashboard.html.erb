 <% content_for :admin_header do %>
    <div class = "div col-t-3 col-d-2" id = "admin-head">
        <%= image_tag "ndu_logo.png" %>
        <div id = "navs">
            <ul class="na">
                <li class = "adminli" id = "dash"><%= image_tag('navs/course.svg', class: 'icon icon-img')%><%= link_to "Dashboard", admin_path, remote:true %></li>
                <li class = "adminli" id = "lect"><%= image_tag('navs/lecturer.svg', class: 'icon icon-img')%><%= link_to "Lecturer", admin_lecturer_path, remote:true %></li>
